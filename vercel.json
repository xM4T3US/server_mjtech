{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
